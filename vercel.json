{
    "version": 2,
    "builds": [
      {
        "src": "potter_store/wsgi.py",
        "use": "@vercel/python"
      },
      {
        "src": "staticfiles/**/*",
        "use": "@vercel/static"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "potter_store/wsgi.py"
      }
    ]
  }
  